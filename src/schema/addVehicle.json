{
    "id": "/AddCustomer",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Add Vehicle",
    "type": "object",
    "additionalProperties": true,
    "required": [
        "id",
        "vehicle Service Type",
        "vehicle Type",
        "Vehicle Number",
        "Registration Place",
        "Current location",
        "No Of Owners"
    ],
    "properties": {
        "id": {
            "type": ["string"],
            "description": "Unique ID"
        },
        "vehicle Service Type": {
            "type":"string" 
        },
        "vehicle Type": {
            "type":"string"
        },
        "Vehicle Number": {
            "type":"string"   
        },
        "Make": {
            "type":"string"
        },
        "Model": {
            "type":"string"
        },
        "Varient": {
           "type":"string"
        },
        "Year": {
          "type":"string",
          "format": "date"
        },
        "Color":{
            "type":"string"
        },
        "Registration Place":{
            "type":"string"
        },
        "Current location":{
            "type":"string"
        },
        "Fuel type":{
            "type":"string"
        },
        "Transmission type":{
            "type":"string"
        },
        "Gares":{
            "type":"string"
        },
        "Price":{
            "type":"string"
        },
        "Vehicle Category":{
            "type":"string"
        },
        "No Of Owners":{
            "type":"number"
        },
        "Engine Capcity CC":{
            "type":"string"
        },
        "Vehicle Full Details": {
            "type": [
                "array"
            ],
            "items":{
                "type":"string"
            },
            "description": "Vehicle  Details"
        },
        "Vehicle Image ID": {
            "type": [
                "array"
            ],
            "items":{
                "type":"string"
            },
            "description": "Vehicle Image Id"
        },
        "createdat": {
            "type": "string",
            "format": "date",
            "description": "Created At Date"
        },
        "CreatedBy": {
            "type": "string"
        }
    }
}